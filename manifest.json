{
    "manifest_version": 3,
    "name": "Bulk Link Opener",
    "version": "1.0",
    "description": "Open multiple links pasted in a text area",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  